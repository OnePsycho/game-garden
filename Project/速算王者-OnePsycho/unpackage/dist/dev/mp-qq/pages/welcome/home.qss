@import '../../common/main.qss';
.page {
  height: 100vh;
  position: relative;
  padding-bottom: 150rpx;
  font-family: 'myFont';
  font-weight: 200;
}
.page .logo {
  width: 100%;
}
.page .logo image {
  width: 80%;
  margin-left: 10%;
  margin-top: 20px;
}
.page .logo .logo-image {
  width: 34%;
  margin-left: 33%;
  margin-top: 15%;
}
.page .btn-start {
  position: absolute;
  width: 100%;
  bottom: 10%;
  left: 0;
}
.page .btn-start .btns-item {
  width: 40%;
  margin-left: 30%;
  border: 1px solid #d1d1d1;
  margin-bottom: 30rpx;
}
.page .btn-start button {
  width: 40%;
  margin-left: 30%;
  padding: 0;
  border: none;
  border-radius: 0;
  height: 119rpx;
  background: none;
}
.page .btn-start button::after {
  border: none;
  border-color: #d1d1d1;
  border-bottom: 1px solid #d1d1d1;
  border-radius: 0;
}
.page .btn-start button image {
  width: 100%;
  border: 1px solid #d1d1d1;
}
.page .btn-start-txt {
  width: 60%;
  margin-left: 20%;
  border: 2px solid #e0e0e0;
  text-align: center;
  height: 150rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 32px;
  color: #7a7a7a;
}
.type-title {
  margin-top: 50rpx;
  font-size: 22px;
}
.cu-dialog {
  padding-bottom: 40rpx;
}
.type-item {
  padding: 20rpx 0;
  margin: 40rpx 0;
  font-size: 18px;
  border: 1px solid #e0e0e0;
  box-shadow: 0 0 5px 2px #626262;
  width: 50%;
  margin-left: 25%;
  background: #868686;
  color: #fff;
}
.type-tips {
  width: 60%;
  margin-left: 20%;
  color: #888888;
}

